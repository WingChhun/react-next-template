{
  "name": "react-next-template",
  "version": "1.0.25",
  "private": true,
  "scripts": {
    "build": "next build",
    "component": "cypress open --component",
    "component:headless": "cypress run --component",
    "dev": "next dev",
    "e2e": "start-server-and-test dev http://localhost:3000 \"cypress open --e2e\"",
    "e2e:headless": "start-server-and-test dev http://localhost:3000 \"cypress run --e2e\"",
    "postinstall": "sort-package-json",
    "lint": "next lint",
    "prettier:write": "npx prettier --write **/*.{ts,tsx,json} --ignore-path .prettierignore",
    "release:minor": "standard-version --no-verify --tag-prefix --release-as minor",
    "release:patch": "standard-version --no-verify --tag-prefix --release-as patch",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest --ci"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && npx pretty-quick",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "@types/node": "20.4.4",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "dotenv": "^16.3.1",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "next": "13.4.12",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/react": "18.0.28",
    "@types/testing-library__jest-dom": "5.14.5",
    "cypress": "12.3.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^4.3.0",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "json-graphql-server": "^2.3.2",
    "json-server": "^0.17.3",
    "prettier": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
    "pretty-quick": "^3.1.3",
    "sort-package-json": "^2.4.1",
    "standard-version": "^9.5.0",
    "start-server-and-test": "1.15.2"
  }
}
